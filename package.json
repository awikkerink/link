{
  "name": "vui-link",
  "version": "2.0.0",
  "description": "Web component link element",
  "private": true,
  "scripts": {
    "prebuild:regular": "cpy link.html ./bower_components/temp && cpy ./test/link-vulcanize-index.html ./bower_components/temp",
    "build:regular": "vulcanize ./bower_components/temp/link-vulcanize-index.html > ./test/perceptual/link.html.ignore",
    "build:example1": "vulcanize ./bower_components/temp/link.html > ./test/perceptual/link-only.html.ignore",
    "build:example2": "vulcanize ./bower_components/vui-typography/typography.html > ./test/perceptual/typography-only.html.ignore",
    "build:example3": "npm run build:example3-typography && npm run build:example3-link",
    "build:example3-typography": "vulcanize ./test/typography-example3-registry.html --strip-exclude=polymer > ./test/perceptual/typography-example3-registry.html.ignore",
    "build:example3-link": "vulcanize ./test/link-example3-registry.html --strip-exclude=\"../bower_components/polymer/polymer.html\" > ./test/perceptual/link-example3-registry.html.ignore",
    "build": "npm run build:regular && npm run build:example1 && npm run build:example2 && npm run build:example3",
    "postbuild": "rimraf ./bower_components/temp",
    "preperceptual-diff": "npm run build",
    "perceptual-diff": "percy snapshot test/perceptual --enable_javascript",
    "postinstall": "bower install",
    "screenshot": "phantomjs ./screenshots/build.js",
    "test": "polylint && wct --skip-plugin sauce"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-link.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/valence-ui-link/issues"
  },
  "homepage": "https://github.com/Brightspace/valence-ui-link",
  "devDependencies": {
    "bower": "^1.7.7",
    "cpy-cli": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.4",
    "polylint": "^2.10.0",
    "rimraf": "^2.5.2",
    "vulcanize": "^1.14.7",
    "web-component-tester": "^4.2.2"
  }
}
